{
  "env": {
    "es6": true,
    "node": true
  },
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "quotes": ["warn", "double"],
    "no-console": "off",
    "no-unused-vars": "off",
    "no-duplicate-imports": "off",
    "class-methods-use-this": "off"
  }
}
